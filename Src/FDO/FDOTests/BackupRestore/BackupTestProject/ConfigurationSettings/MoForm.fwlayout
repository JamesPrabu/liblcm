<LayoutInventory>
  <layout class="MoForm" type="jtview" name="publishStemForm" version="9">
	<part ref="MorphTypeConfig" label="Morph Type" before="" after=" " visibility="never" ws="analysis" wsType="analysis" param="publishStemMorphType" sep=" " />
	<part ref="FormPub" label="Allomorph" before="" after=" " visibility="never" ws="vernacular" wsType="vernacular" sep=" " />
	<part ref="FormEnvPub" label="Environments" after=" " visibility="never" sep=", " />
	<generate class="MoForm" fieldType="mlstring" restrictions="customOnly">
	  <part ref="$child" label="$label" before="" after=" " visibility="never" ws="$wsName" wsType="analysis vernacular" sep=" " showLabels="false">
		<configureMlString field="$fieldName" />
	  </part>
	</generate>
	<generate class="MoForm" fieldType="simplestring" restrictions="customOnly">
	  <part ref="$child" label="$label" before="" after=" " visibility="never">
		<string field="$fieldName" />
	  </part>
	</generate>
  </layout>
  <layout class="MoForm" type="jtview" name="publishStemLexemeForm" version="9">
